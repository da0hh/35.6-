#include <vector>
#include <iostream>
#include <string>
#include <initializer_list>

using namespace std;

void printVec(initializer_list<int> list) {
    for (auto i : list)
        cout << i << " ";
    cout << endl;
}

int main(){
    setlocale(LC_ALL, "RU");
    vector<int> vec = { 1,2,3,4,5 };
    printVec({ 1,2,3,4,5 });
    return 0;
}
