#include <vector>
#include <iostream>
#include <string>
#include <unordered_set>

using namespace std;

int main(){
    setlocale(LC_ALL, "RU");
    auto filtrUnique = [](vector<int> vec) {
        unordered_set<int>exception;
        auto res = make_unique<vector<int>>();
        for (auto i : vec) {
            if (!exception.contains(i)) {
                exception.emplace(i);
                res->push_back(i);
            }
        }
        
        return res;
        };

    vector<int> numbers = { 1, 2, 3, 2, 4, 1, 5, 5, 6 };

    unique_ptr<vector<int>> uniqueNumbers = filtrUnique(numbers);

    for (int num : *uniqueNumbers)
        cout << num << " ";

    return 0;
}
